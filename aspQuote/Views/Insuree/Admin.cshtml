@model IEnumerable<aspQuote.Models.Insuree>

@{
    ViewBag.Title = "Admin";
}


<h2>Admin - All Issued Quotes</h2>

<table class="table">
    <thead>
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Email Address</th>
            <th>Quote</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@Html.DisplayFor(modelItem => item.FirstName)</td>
                <td>@Html.DisplayFor(modelItem => item.LastName)</td>
                <td>@Html.DisplayFor(modelItem => item.EmailAddress)</td>
                <td>@Html.DisplayFor(modelItem => item.Quote)</td>
            </tr>
        }
    </tbody>
    <tbody>
    <td>ASD</td>
    <td>ASD</td>
    <td>asd</td>
    <td>asd</td>
    <td>asd</td>
    </tbody>
</table>
    }
}


